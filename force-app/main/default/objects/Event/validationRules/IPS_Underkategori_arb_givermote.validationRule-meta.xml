<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IPS_Underkategori_arb_givermote</fullName>
    <active>true</active>
    <description>Når man legger inn arbeidsgiver i et møte så skal også underkategori fylles ut</description>
    <errorConditionFormula>AND(
 RecordType.DeveloperName = &quot;IPS_Event&quot;,
 NOT(ISBLANK(RelatedToAccount__c )),
ISBLANK(Text(ips_Subcategory_Type_IPS__c))
)</errorConditionFormula>
    <errorDisplayField>ips_Subcategory_Type_IPS__c</errorDisplayField>
    <errorMessage>Underkategori må legges til !</errorMessage>
</ValidationRule>

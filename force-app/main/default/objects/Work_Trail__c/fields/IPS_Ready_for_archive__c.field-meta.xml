<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IPS_Ready_for_archive__c</fullName>
    <description>Checked when the Worktrail is eligible for archiving  - 1 year after the end date.</description>
    <formula>IF( ( ips_End_Date__c  + 373 ) &gt;= TODAY() ,
TRUE,
FALSE)</formula>
    <label>Ready for archive</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>

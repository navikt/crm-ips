            <name>Check_IfWorkTrailIsReadOnly_HasClosedDate</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>sObj_selectedWorkTrail.Oppstartsdato__c</leftValueReference>
                <operator>WasSet</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <isGoTo>true</isGoTo>
                <targetReference>Screen_OfferOpeningWorkTrailForEditing</targetReference>
            </connector>
            <label>Work Trail Has Closed Date</label>
        </rules>

        </defaultConnector>
        <defaultConnectorLabel>Work Trail is open for editing</defaultConnectorLabel>
        <rules>
            <name>Check_IfWorkTrailIsReadOnly_Yes</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>sObj_selectedWorkTrail.IPS_IsEndCauseSet__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sObj_selectedWorkTrail.ips_Status__c</leftValueReference>
                <operator>EqualTo</operator>

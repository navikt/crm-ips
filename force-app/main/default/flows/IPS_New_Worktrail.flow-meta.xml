    <formulas>
        <name>adjustfor100</name>
        <dataType>Number</dataType>
        <expression>if({!participantsAge}&lt;0,
({!participantsAge}+100),
{!participantsAge})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>participantDOB</name>
        <dataType>Date</dataType>
        <expression>IF(NOT(ISBLANK({!getPersonDetail.Name})),
DATE(
VALUE(&quot;20&quot;&amp;MID({!getPersonDetail.Name},5,2)),
VALUE(MID({!getPersonDetail.Name},3,2)),
VALUE(MID({!getPersonDetail.Name},1,2))
),null)</expression>
    </formulas>
    <formulas>
        <name>participantsAge</name>
        <dataType>Number</dataType>
        <expression>IF(NOT(ISBLANK({!participantDOB} )) ,
IF( DATE( YEAR(TODAY()), MONTH({!participantDOB}) , DAY({!participantDOB})) &lt;= DATE( YEAR(TODAY()) , MONTH( TODAY() ) , DAY( TODAY() ) ),
     YEAR (Today()) - YEAR ( {!participantDOB}),
     YEAR (Today()) - YEAR ( {!participantDOB}) -1 ),
0)</expression>
        <scale>0</scale>
    </formulas>

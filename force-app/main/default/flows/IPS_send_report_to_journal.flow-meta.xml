    <description>Send IPS/SE report to Joark</description>
    <environments>Default</environments>
    <formulas>
        <description>Formula to define the journal case type (Sakstype). If no Journal Case ID has been input, default is GENERELL_SAK</description>
        <name>Journal_Case_Type</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!Journal_Case_ID}), &apos;GENERELL_SAK&apos;, &apos;FAGSAK&apos;)</expression>
    </formulas>
    <formulas>
        <description>Formula to generate name convention on journal required format (For persons)</description>
        <name>Person_Full_Name</name>
        <dataType>String</dataType>
        <expression>{!Get_Account_Info.CRM_Person__r.INT_LastName__c} &amp; &apos;, &apos; &amp; {!Get_Account_Info.CRM_Person__r.INT_FirstName__c} &amp; &apos; &apos; &amp;{!Get_Account_Info.CRM_Person__r.INT_MiddleName__c}</expression>
    </formulas>
    <formulas>
        <description>Tester på om rapport er en sluttrapport, hvis ikke er antagelsen at det er en underveisrapport.
Case setter tittel basert på om det er IPS eller UO</description>
        <name>varDocumentTitle</name>
        <dataType>String</dataType>
        <expression>IF({!varEnd},
CASE({!varRecordTypeName},
&quot;ips_Supported_Employment&quot;,&quot;Sluttrapport Utvidet oppfølging&quot;,
&quot;IPS&quot;,&quot;Sluttnotat IPS&quot;,
&quot;&quot;),
CASE({!varRecordTypeName},
&quot;ips_Supported_Employment&quot;,&quot;Underveisrapport Utvidet oppfølging&quot;,
&quot;IPS&quot;,&quot;Underveisnotat IPS&quot;,
&quot;&quot;))</expression>
    </formulas>
    <formulas>
        <name>varUserNavIdent</name>
        <dataType>String</dataType>
        <expression>{!$User.CRM_NAV_Ident__c}</expression>
    </formulas>

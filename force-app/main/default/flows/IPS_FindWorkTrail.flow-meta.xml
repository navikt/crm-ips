    </customProperties>
    <decisions>
        <name>Check_IfUserWantToReopenWorktrail</name>
        <label>Check If User Want To Reopen Worktrail</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Screen_NavigtateToSelectedRecord</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>User Does Not Want To Reopen Work Trail</defaultConnectorLabel>
        <rules>
            <name>Check_IfUserWantToReopenWorktrail_Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_OpenForEditing.value</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_EmptyEndDetails</targetReference>
            </connector>
            <label>Users Want To Reopen Worktrail</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_IfWorkTrialReadOnly</name>
        <label>Check If Work Trial Read-Only</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Screen_NavigtateToSelectedRecord</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Work Trail is open for editing</defaultConnectorLabel>
        <rules>
            <name>Check_IfWorkTrialReadOnly_Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>sObj_selectedWorkTrail.IPS_IsEndCauseSet__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sObj_selectedWorkTrail.ips_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Ended</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Screen_OfferOpeningWorkTrailForEditing</targetReference>
            </connector>
            <label>Work Trail is Read-Only</label>
        </rules>
    </decisions>
    <description>Helps admin users find a Job Trail by the person id of the participant.
If the Work Trail is ended and closed for editing, we offer to open it.</description>
    <environments>Default</environments>
    <interviewLabel>IPS - Find Work Trail {!$Flow.CurrentDateTime}</interviewLabel>
    <label>IPS - Find Work Trail</label>

        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Assign_WorkTrail</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Check_IfWorkTrialReadOnly</targetReference>
        </noMoreValuesConnector>
    </loops>
